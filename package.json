{
    "name": "verilog-testbench-instance",
    "displayName": "Verilog_Testbench",
    "description": "verilog-testbench-instance",
    "version": "0.0.1",
    "publisher": "Truecrab",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.instance",
        "onCommand:extension.testbench"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.instance",
                "title": "Instance"
            },{
                "command": "extension.testbench",
                "title": "Testbench"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
